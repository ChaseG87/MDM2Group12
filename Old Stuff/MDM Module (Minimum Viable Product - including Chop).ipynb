    "## Importing Modules\n",
    "import pandas as pd\n",
    "\n",
    "'''\n",
    "This is the compact version of the Minimum Viable Product - only the most core code.\n",
    "Assumes correct brackets, and takes a function and applies it to another function.\n",
    "It then simplifies this function.\n",
    "It is also capable of basic file reading, and can take compound functions of length 2, and apply them recursively. <UNTESTED>\n",
    "\n",
    "\n",
    "Method:\n",
    "1. Identify functions and expressions as classes\n",
    "2. Evaluate application of innermost objects on each other\n",
    "3. Evaluation alpha conversion, then beta, then <eta> reduction (maybe)\n",
    "4. Repeat 2 and 3 until it cannot be done anymore\n",
    "\n",
    "Areas for future expansion (* indicates key importance):\n",
    "- Eta reduction\n",
    "- Diagnostics\n",
    "- Multiple Lambda Calclus inputs*\n",
    "- Compound functions of length 3*\n",
    "- Linking File Reading to Compound Function Solver*\n",
    "'''\n",
    "\n",
    "def pull_txt(filename = \"hello.txt\"):\n",
    "    df = pd.read_csv(filename, sep=\" \", header=None)\n",
    "    sentence = df[0][0] \n",
    "    return sentence\n",
    "\n",
    "def chop(sentence):\n",
    "    '''chops parentheses'''\n",
    "\n",
    "    def check_parenthesis(sentence):\n",
    "        if sentence.count('(') != sentence.count(')'):\n",
    "            raise ValueError('The sentence has incorrect parenthetic grammar.')\n",
    "\n",
    "\n",
    "    def parse_expression(sentence, index=0):\n",
    "        '''Recursive parser'''\n",
    "        result = []\n",
    "        current = \"\"\n",
    "\n",
    "        while index < len(sentence):\n",
    "            char = sentence[index]\n",
    "            if char == '(':\n",
    "                if current:\n",
    "                    result.append(current)\n",
    "                    current = ''\n",
    "                nested, index = parse_expression(sentence, index + 1) #Recursive step\n",
    "                result.append(nested)\n",
    "            elif char == ')':\n",
    "                if current:\n",
    "                    result.append(current)\n",
    "                return result, index #stops sub operations\n",
    "            else:\n",
    "                current += char\n",
    "            index += 1\n",
    "        if current:\n",
    "            result.append(current)\n",
    "        return result, index\n",
    "\n",
    "    check_parenthesis(sentence)\n",
    "    parsed = parse_expression(sentence)[0]\n",
    "    return parsed\n",
    "\n",
    "def preprocess1(sentence):\n",
    "    '''takes in a function and puts it in correct form eg. /x./y.xy -> /x.(/y.xy)'''\n",
    "    for idx in range(len(sentence)):\n",
    "        char = sentence[idx]\n",
    "        if char == '.':\n",
    "            if sentence[idx+1] == '/':\n",
    "                paren_count = 0\n",
    "                pos = idx+1\n",
    "                while paren_count >= 0 and pos <= len(sentence)-1:\n",
    "                    if sentence[pos] == '(':\n",
    "                        paren_count += 1\n",
    "                    if sentence[pos] == ')':\n",
    "                        paren_count -= 1\n",
    "                    pos += 1\n",
    "                sentence = sentence[0:idx+1] + '(' + sentence[idx+1:pos] + ')' + sentence[pos:]\n",
    "    return sentence\n",
    "\n",
    "class Function:\n",
    "    def __init__(self, string):\n",
    "        period_index = string.index('.')\n",
    "        self.vars = string[1:period_index]\n",
    "        self.body = string[period_index+1:]\n",
    "        self.string = string\n",
    "\n",
    "    def __call__(self):
    "       """
    "       Runs a Diagnostics - Useful for Displaying.
    "       """
    "       print("String", self.string)
    "       print("Body", self.body)
    "       print("Vars", self.vars,"\n")
    "   
    "    def beta_reduce(self, inputs):\n",
    "        '''\n",
    "        inputs is a list of each individual input\n",
    "        This does the beta reduction using the relative inputs.\n",
    "        '''\n",
    "\n",
    "        def alpha_conversion(input, body):\n",
    "            '''\n",
    "            Changes input into form that doesnt conflict with body via alpha conversion.\n",
    "            '''\n",
    "\n",
    "            def find_replacement(input, body):\n",
    "                \"\"\" Finds a Substitute Variable - Used in the Alpha Reduction Function\"\"\"\n",
    "                \n",
    "                option_list = 'abcdefghijklmnopqrstuvwxyz'\n",
    "                for option in option_list:\n",
    "                    if option not in input and option not in body:\n",
    "                        return option\n",
    "        \n",
    "            for char in input:\n",
    "                if char not in '/.()' and char in body:\n",
    "                    replacement = find_replacement(input, body)\n",
    "                    for i in range(input.count(char)):\n",
    "                        rep_index = input.index(char)\n",
    "                        input = input[:rep_index] + replacement + input[rep_index+1:]\n",
    "            return input\n",
    "\n",
    "\n",
    "        var_list = [self.vars[i] for i in range(len(self.vars))]\n",
    "        ins = ['(' + input.string + ')' if type(input) == Function else input for input in inputs]\n",
    "\n",
    "        for index in range(len(ins)):           \n",
    "            var = var_list[index]\n",
    "            self.vars = self.vars[1:]\n",
    "            innie = ins[index]\n",
    "            innie = alpha_conversion(innie, self.body)\n",
    "\n",
    "            for i in range(self.body.count(var)):\n",
    "                insert_index = self.body.index(var)\n",
    "                self.body = self.body[0:insert_index] + innie + self.body[insert_index+1:]\n",
    "\n",
    "def application_of_chop(input):\n",
    "    \"\"\"\n",
    "    Applies the function recursively to chopped function <see chop function for further details>.\n",
    "    This makes sure that the reader can parse compound functions.\n",
    "    \"\"\"\n",
    "    ## NEED TO ADD A MODE FOR 3\n",
    "    if len(input) == 2:\n",
    "        if len(function1) != 1:\n",
    "            application_of_chop(function1)\n",
    "        if len(function2) != 1:\n",
    "            application_of_chop(function2)\n",
    "\n",
    "        function1 = Function(input[0])\n",
    "        function2 = Function(input[1])\n",
    "        return function1.beta_reduce([function2])\n",
    "    \n",
    "def main():\n",
    "    sentence = pull_txt()\n",
    "    sentence = preprocess1(sentence)\n",
    "    chopped = chop(sentence)\n",
    "    if len(chopped) == 2:\n",
    "        application_of_chop(chopped)\n",
    "    print(chopped)\n",
    "    print(sentence)\n",
    "\n",
    "## RUNNING CODE\n",
    "sent1 = Function('/xy.xxyy')\n",
    "sent2 = 'yy'\n",
    "sent3 = Function('/bc.c')\n",
    "\n",
    "sent1.beta_reduce([sent3])\n",
    "sent1()\n",
    "\n",
    "sent1.beta_reduce([sent2])\n",
    "sent1()"
   ]
